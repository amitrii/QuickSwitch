{
    "manifest_version": 3,
    "name": "Quick",
    "version": "1.0",
    "description": "Toggle between CMS and live site for the current tab.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "icons/icon48.png"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "browsingData"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  